{
    "printWidth": 80,
    "trailingComma": "none",
    "tabWidth": 4,
    "useTabs": false,
    "react/jsx-max-props-per-line": [
        1,
        {
            "when": "always"
        }
    ],
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "arrowParens": "always"
}
